{
	"name": "itlo/cms-basic",
	"description": "ITLO CMS basic package",
	"keywords": ["yii", "itlo", "framework", "component"],
	"homepage": "https://cms.itlo.ru/",
	"type": "yii2-extension",
	"license": "BSD-3-Clause",
	"support": {
		"issues": "https://itlo.com/",
		"wiki": "https://cms.itlo.com/docs",
		"source": "https://github.com/itlo-cms/cms-basic.git"
	},

	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"authors": [
		{
			"name": "Ivan  Roman",
			"email": "info@itlo.ru"
		}
	],
	"require": {
		"itlo/cms": "^0.0.1 || dev-master"
	},
	"autoload": {
		"psr-4": {
			"skeeks\\cms\\cmsbasic\\": "src/"
		}
	},
	"extra": {
		"config-plugin": {
			"web": [
				"src/config/common.php",
				"src/config/web.php"
			],
			"console": [
				"src/config/common.php",
				"src/config/console.php"
			]
		}
	}
}
